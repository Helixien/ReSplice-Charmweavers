	</GeneDef>
	<!-- ================ Libido ================ -->
	<GeneDef ParentName="LibidoBase">
		<defName>RS_Libido_Extreme</defName>
		<label>insatiable libido</label>
		<description>Carriers of this gene are extremely likely to engage in lovin' with their partner.</description>
		<iconPath>UI/Icons/Genes/Gene_InsatiableLibido</iconPath>
		<customEffectDescriptions>
			<li>Highly increases the chance of lovin'.</li>
		</customEffectDescriptions>
		<displayOrderInCategory>20</displayOrderInCategory>
		<lovinMTBFactor>0.1</lovinMTBFactor>
	</GeneDef>
	<GeneDef ParentName="LibidoBase">
		<defName>RS_Libido_None</defName>
		<label>no libido</label>
		<description>Carriers of this gene are extremely likely to engage in lovin' with their partner.</description>
		<iconPath>UI/Icons/Genes/Gene_InsatiableLibido</iconPath>
		<customEffectDescriptions>
			<li>Highly increases the chance of lovin'.</li>
		</customEffectDescriptions>
	</GeneDef>
	<!-- ================ Beauty ================ -->
	<GeneDef ParentName="GeneBeautyBase">
		<defName>RS_Beauty_Angelic</defName>
		<label>extremely attractive</label>
		<description>Carriers of this gene have remarkably precise and symmetrical faces. Their features are distinctive and strong without being exaggerated, and their skin is nearly perfect. They are generally seen as beautiful.</description>
		<iconPath>UI/Icons/Genes/Gene_Extremely attractive</iconPath>
		<displayOrderInCategory>30</displayOrderInCategory>
		<statOffsets>
			<PawnBeauty>2</PawnBeauty>
		</statOffsets>
		<biostatMet>-2</biostatMet>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>charm</symbol>
				</li>
				<li>
					<symbol>ele</symbol>
				</li>
				<li>
					<symbol>grace</symbol>
				</li>
				<li>
					<symbol>neat</symbol>
				</li>
				<li>
					<symbol>cute</symbol>
				</li>
				<li>
					<symbol>beauti</symbol>
				</li>
				<li>
					<symbol>smooth</symbol>
				</li>
			</prefixSymbols>
		</symbolPack>
	</GeneDef>
