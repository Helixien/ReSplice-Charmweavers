	<GeneDef>
		<defName>RS_Matrianic</defName>
		<label>matrianic</label>
		<description>Offspring of the carrier of this gene are 90% of the time the same gender as the mother.</description>
		<iconPath>UI/Icons/Genes/Gene_Matrianic</iconPath>
		<displayCategory>Archite</displayCategory>
		<biostatMet>-1</biostatMet>
		<biostatArc>1</biostatArc>
	</GeneDef>
	<GeneDef>
		<defName>RS_Superfecundation</defName>
